{
  "title": "YouTube Video Downloader Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "videoUrls": {
      "title": "YouTube Video URLs",
      "type": "array",
      "editor": "stringList",
      "description": "Enter a list of YouTube video URLs to download.",
      "prefill": [
        "https://www.youtube.com/watch?v=dQw4w9WgXcQ"
      ]
    },
    "quality": {
      "title": "Video Quality",
      "type": "string",
      "editor": "select",
      "description": "Select the desired video quality.",
      "default": "best",
      "options": [
        "best",
        "worst",
        "bestvideo[ext=mp4]+bestaudio[ext=m4a]/best[ext=mp4]/best",
        "bestvideo[height<=1080]+bestaudio/best[height<=1080]",
        "bestvideo[height<=720]+bestaudio/best[height<=720]",
        "bestvideo[height<=480]+bestaudio/best[height<=480]",
        "bestvideo[height<=360]+bestaudio/best[height<=360]"
      ]
    },
    "proxyType": {
      "title": "Proxy Group (or NONE)",
      "type": "string",
      "editor": "textfield",
      "description": "Enter the exact name of your proxy group from the Apify dashboard (e.g., RESIDENTIAL). To disable proxies, enter NONE.",
      "default": "RESIDENTIAL"
    }
  },
  "required": [
    "videoUrls"
  ]
}